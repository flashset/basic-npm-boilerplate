{
  "name": "basic-npm-boilerplate",
  "version": "1.0.2",
  "private": false,
  "main": "dist/index.js",
  "homepage": "http://www.sparkyflash.com/tech",
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "react": "17.0.2",
    "react-scripts": "^4.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepublish": "npm run build && babel ./src -d dist --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/flashset/basic-npm-boilerplate.git"
  },
  "keywords": [
    "react",
    "sparkyflash"
  ],
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-react": "^7.13.13"
  }
}
